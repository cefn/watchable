{
  "name": "counter-react-js",
  "version": "0.0.1-alpha.1",
  "private": true,
  "homepage": "https://github.com/cefn/watchable/tree/main/modules/store#readme",
  "bugs": {
    "url": "https://github.com/cefn/watchable/issues",
    "email": "watchable@cefn.com"
  },
  "repository": "github:cefn/watchable",
  "license": "MIT",
  "author": "Cefn Hoile <github.com@cefn.com> (https://cefn.com)",
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "wireit"
  },
  "dependencies": {
    "@lauf/store": "^1.1.2-alpha.0",
    "@lauf/store-react": "^1.1.2-alpha.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "vite": "^3.2.3",
    "wireit": "^0.8.0"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../../packages/store:build",
        "../../packages/store-react:build"
      ],
      "command": "vite build",
      "files": [
        "src/**/*"
      ],
      "output": [
        "./dist"
      ]
    },
    "lint": {
      "command": "eslint -c .eslintrc.cjs .",
      "files": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.jsx"
      ],
      "output": [],
      "dependencies": [
        "build"
      ]
    }
  }
}